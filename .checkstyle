<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <local-check-config name="cosmos_sdk" location="cosmos_checks.xml" type="project" description="">
        <property name="checkstyle.header.file" value="${project_loc}/java.header"/>
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="cosmos_sdk" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
</fileset-config>
